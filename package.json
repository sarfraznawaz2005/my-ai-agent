{
  "name": "hey-ai-cli",
  "version": "1.0.0",
  "description": "A CLI-based app for non-interactive inference from popular AI CLI tools such as Claude Code, Gemini CLI, Codex, OpenCode, etc.",
  "main": "dist/index.js",
  "bin": {
    "ai": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "keywords": [
    "cli",
    "ai",
    "tools",
    "manager",
    "claude",
    "gemini",
    "opencode",
    "codex",
    "automation"
  ],
  "author": "Sarfraz Ahmed <sarfraznawaz2005@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sarfraznawaz2005/heyai.git"
  },
  "bugs": {
    "url": "https://github.com/sarfraznawaz2005/heyai/issues"
  },
  "homepage": "https://github.com/sarfraznawaz2005/heyai#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.3",
    "commander": "^11.1.0",
    "conf": "^10.2.0",
    "dayjs": "^1.11.10",
    "execa": "^5.1.1",
    "fuse.js": "^7.0.0",
    "inquirer": "^8.2.6",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.10",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
